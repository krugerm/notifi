{
  "scan": ".",
  "include": [],
  "exclude": [
    "frontend/node_modules",
    "frontend/public",
    "frontend/.next",
    "backend/uploads",
    "backend/node_modules",
    ".git",
    ".gitignore",
    ".temp",
    "next-env.d.ts",
    "next.config.ts"
  ],
  "excludeFiles": [
    "backend/package.json",
    "backend/package-lock.json",
    "backend/pubspec.lock",
    "frontend/package.json",
    "frontend/package-lock.json",
    "frontend/pubspec.lock"
  ],
  "extensions": [
    ".html",
    ".tsx",
    ".ts",
    ".jsx",
    ".md",
    ".css"
  ],
  "showExcluded": false,
  "showSkipped": false,
  "outputFile": "v-output.txt"
}
